    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.8.0"
